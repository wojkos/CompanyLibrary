!!!
%html
  %head
    %title CompanyLibrary
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    %body
      .wrapper
        / Sidebar Holder
        %nav#sidebar
          .sidebar-header
            %h3 Bootstrap Sidebar
          %ul.list-unstyled.components
            %p Dummy Heading
            %li.active
              %a{"aria-expanded" => "false", "data-toggle" => "collapse", :href => "#homeSubmenu"} Home
              %ul#homeSubmenu.collapse.list-unstyled
                %li
                  %a{:href => "#"} Home 1
                %li
                  %a{:href => "#"} Home 2
                %li
                  %a{:href => "#"} Home 3
            %li
              %a{:href => "#"} About
              %a{"aria-expanded" => "false", "data-toggle" => "collapse", :href => "#pageSubmenu"} Pages
              %ul#pageSubmenu.collapse.list-unstyled
                %li
                  %a{:href => "#"} Page 1
                %li
                  %a{:href => "#"} Page 2
                %li
                  %a{:href => "#"} Page 3
            %li
              %a{:href => "#"} Portfolio
            %li
              %a{:href => "#"} Contact
          %ul.list-unstyled.CTAs
            %li
              %a.download{:href => "https://bootstrapious.com/tutorial/files/sidebar.zip"} Download source
            %li
              %a.article{:href => "https://bootstrapious.com/p/bootstrap-sidebar"} Back to article
        / Page Content Holder
        #content
          %nav.navbar.navbar-default
            .container-fluid
              .navbar-header
                %button#sidebarCollapse.btn.btn-info.navbar-btn{:type => "button"}
                  %i.glyphicon.glyphicon-align-left
                  %span Toggle Sidebar
              #bs-example-navbar-collapse-1.collapse.navbar-collapse
                %ul.nav.navbar-nav.navbar-right
                  %li
                    %a{:href => "#"} Page
                  %li
                    %a{:href => "#"} Page
                  %li
                    %a{:href => "#"} Page
                  %li
                    %a{:href => "#"} Page
          .container
            %h2 Collapsible Sidebar Using Bootstrap 3
         
